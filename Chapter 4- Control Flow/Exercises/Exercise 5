favorite_fruits = ['banana', 'mango', 'lemon']

if 'lemon' in favorite_fruits:
    print("You really like lemon!")
if 'banana' in favorite_fruits:
    print("I see that you seek protein?")
if 'mango' in favorite_fruits:
    print("You seem to miss the Philippines!")
if 'citrus fruit' in favorite_fruits:
    print("You really like calamnsi!")
if 'orange' in favorite_fruits:
    print("You really like oranges!")